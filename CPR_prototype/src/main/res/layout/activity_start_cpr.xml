<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/backgroundimage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="se.mah.interaction.design.StartCPR"
    tools:ignore="MergeRootFrame"
    android:background="@drawable/manuncons"
    >


    <Button
        android:alpha="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/mouthpress"
        android:layout_marginTop="86dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/chestpress"
        android:layout_alignStart="@+id/chestpress" />

    <Button
        android:alpha="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/chestpress"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/mouthpress"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
